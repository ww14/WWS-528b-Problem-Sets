rename Y1 id
// Get replicate numbers
gen repl = mod(id,10)
replace id = (id-repl)/10
rename X42001 weight
rename X14 age
rename X8023 marstat

// CREDIT CARD DEBT
/* Recode zeros to zeros, missing is zero as well. Topcoding is 999,999 and is 
not binding */
replace X413 = 0 if X413 == -1
replace X421 = 0 if X421 == -1
replace X424 = 0 if X424 == -1
replace X427 = 0 if X427 == -1
replace X430 = 0 if X430 == -1
gen Credit = X413 + X421 + X424 + X427 + X430 + X7575
drop X413 X421 X424 X427 X430 X7575
label variable Credit "Credit card debt"

// Interest rate paid on credit card with the highest balance
rename X7132 ccintrate
label variable ccintrate "Interest rate charged on credit card with highest balance"

// Pay off regularly or not
rename X432 freqpayoff
label define freqpayoff 0 "Inap" 1 "Always or almost always" 3 "Sometimes" 5 "Hardly ever"
label values freqpayoff freqpayoff

// FARMLAND WITH BUSINESS
/* X513 is owned farmland and top coded at 99,999,999 (not binding). X526 is 
the value of the share of partly owned farmland and topcoded at 99,999,999 (not 
binding)
*/
gen Farm = X513 + X526
label variable Farm "Value of fully or partially owned farm"
drop X513 X526

// MOBILE HOME
/* X604 is the value of site and top coded at 999,999 (not binding).
*/
gen Mobilehome = X604 + X614 + X623
label variable Mobilehome "Value of mobile home and/or site"
drop X604 X614 X623

// HOME
/* X716 is the value of home, incl farm not operated as business. X805 and X905 
are first and second mortgage, respectively, and X1005 is other home equity loan */
gen Home = X716 
label variable Home "Value of home"
drop X716
gen Mortgage = X805 + X905 + X1005 + X1044
label variable Mortgage "Total home loans incl mortgages, other home equity loans and other property loans"
rename X805 mortgage1
rename X905 mortgage2
rename X1005 mortgage3
rename X1044 mortgage4
// Interest rate is coded -1 for zero, 0 for missing
replace X816 = . if X816 == 0
replace X816 = 0 if X816 == -1
replace X916 = . if X916 == 0
replace X916 = 0 if X916 == -1
replace X1016 = . if X1016 == 0
replace X1016 = 0 if X1016 == -1
replace X1045 = . if X1045 == 0
replace X1045 = 0 if X1045 == -1
rename X816 hmrate1
rename X916 hmrate2
rename X1016 hmrate3
rename X1045 hmrate4
label variable hmrate1 "Interest rate charged on first mortgage"
label variable hmrate2 "Interest rate charged on second mortgage"
label variable hmrate3 "Interest rate charged on other home equity loan"
label variable hmrate4 "Interest rate charged on other property loan"

// LINES OF CREDIT
/* X1108, X1119 and X1130 are first, second and third lines of credit, X1136 is
all remaining lines of credit */
gen Cline = X1108 + X1119 + X1130 + X1136
