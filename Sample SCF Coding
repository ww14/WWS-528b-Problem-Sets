replace X4731 = X4731*2 if X4732 == 11
replace X4131 = X4131*6 if X4132 == 12
replace X4731 = X4731*6 if X4732 == 12
replace X4131 = X4131*hours*weeks if X4132 == 18
replace X4731 = X4731*hours_s*weeks_s if X4732 == 18
